{
  "$schema": "https://json.schemastore.org/swcrc",
  "exclude": "node_modules",
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "parser": {
      "tsx": false,
      "syntax": "typescript",
      "decorators": true
    },
  
    "target": "es2020",
    "keepClassNames": true,
    "baseUrl": "./",
    "paths": {
      "database": ["database/*"]
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    }
  }
}
